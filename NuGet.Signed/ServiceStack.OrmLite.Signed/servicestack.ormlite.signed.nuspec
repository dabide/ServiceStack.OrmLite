    <tags>OrmLite POCO Code-First Orm Schema-less Blobs</tags>
    <language>en-US</language> 
    <copyright>ServiceStack 2013 and contributors</copyright>
    <dependencies>
      <dependency id="ServiceStack.Common.Signed" version="4.0" />
    </dependencies> 
  </metadata> 
